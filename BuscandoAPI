# Aqui apenas testando como funciona a lógica dessa API
import requests
url = 'http://localhost:3000/ID_Cliente'
response = requests.get(url)
if response.status_code == 200:
    data = response.json()    
    print(data)
else:
    print(f'Erro na requisição. Código de status: {response.status_code}')
